{
  "name": "minimum-react-ts",
  "version": "1.0.0",
  "scripts": {
    "prebuild": "tsc --noEmit",
    "build": "webpack",
    "start": "webpack serve",
    "test": "jest",
    "test:ci": "jest --coverage --updateSnapshot --ci",
    "test:e2e": "cypress open",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "prettier": "prettier --write '**/*.{js,ts,tsx,md,json,html,scss}'",
    "prettier:check": "yarn prettier --check",
    "codecheck": "yarn lint && yarn prettier:check",
    "codefix": "yarn lint:fix && yarn prettier",
    "postinstall": "make postinstall"
  },
  "main": "index.js",
  "repository": "https://github.com/flyhighair/minimum-react-ts.git",
  "author": "hakshu <flyhighup.air25@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">= 14"
  },
  "dependencies": {
    "@apollo/client": "^3.3.15",
    "graphql": "^15.5.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "7.14.2",
    "@babel/preset-react": "7.13.13",
    "@types/jest": "26.0.23",
    "@types/react": "17.0.5",
    "@types/react-dom": "17.0.5",
    "@types/react-test-renderer": "17.0.1",
    "@typescript-eslint/eslint-plugin": "4.23.0",
    "@typescript-eslint/parser": "4.23.0",
    "babel-jest": "27.0.1",
    "css-loader": "5.2.4",
    "cypress": "7.3.0",
    "esbuild-loader": "2.13.0",
    "eslint": "7.27.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-react": "7.23.2",
    "html-webpack-plugin": "5.3.1",
    "husky": "6.0.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "27.0.1",
    "lint-staged": "11.0.0",
    "prettier": "2.2.1",
    "react-test-renderer": "17.0.2",
    "sass": "1.32.13",
    "sass-loader": "11.1.1",
    "style-loader": "2.0.0",
    "ts-jest": "26.5.6",
    "typescript": "4.2.4",
    "webpack": "5.37.0",
    "webpack-cli": "4.7.0",
    "webpack-dev-server": "3.11.2"
  },
  "resolutions": {
    "node-forge": "0.10.0"
  }
}
